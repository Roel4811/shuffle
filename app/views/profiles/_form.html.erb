<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
    <%= simple_form_for @profile do |f| %>

        <%= f.input :name %>
        <%= f.input :age %>
        <%= f.input :location %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>

      <%= f.submit "Go", class: "btn btn-primary" %>
    <% end %>

    </div>
  </div>
</div>




